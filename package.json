{
  "name": "komoot-plus",
  "version": "1.0.0",
  "description": "Chrome extensions adding visualization features to Komoot",
  "private": true,
  "scripts": {
    "server:dev": "cross-env WDS=true webpack-dev-server --config webpack.config.dev.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build:prod": "webpack --config webpack.config.prod.js",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch"
  },
  "keywords": [
    "komoot",
    "chrome-extension"
  ],
  "author": "Micha≈Ç Tkacz",
  "license": "MIT",
