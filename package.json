    "@types/react-resizable": "^1.7.4",
    "deck.gl": "^8.7.3",
    "mapbox-gl": "npm:empty-npm-package@^1.0.0",
    "maplibre-gl": "^2.1.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-map-gl": "^7.0.10",
    "react-resizable": "^3.0.4",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
